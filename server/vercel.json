{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "DB_HOST": "your_db_host",
      "DB_USER": "your_db_user",
      "DB_PASSWORD": "your_db_password",
      "DB_NAME": "your_db_name",
      "JWT_SECRET": "your_jwt_secret",
      "EMAIL_USER": "your_email_user",
      "EMAIL_PASSWORD": "your_email_password"
    }
  }